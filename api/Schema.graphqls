schema {
    query: Query
}

type Query {
    me: User
    users: [User]
}

type User {
    id: ID!
    name: String!
    email: String!

    friends: [User]
    comments: [Comment]
    photos: [Photo]
}

type Comment {
    id: ID!
    text: String!
}

type Photo {
    id: ID!
    src: String!
    owner: User!
}
